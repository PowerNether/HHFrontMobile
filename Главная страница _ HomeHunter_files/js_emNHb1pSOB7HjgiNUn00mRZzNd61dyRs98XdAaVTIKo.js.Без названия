        data: 'data=' + $.toJSON(info),        success: function(data) {          $('.file-box .cssload-container').remove();          $('.page-xml-validator .result-box').remove();          $('input[name="xml"]').removeClass('error').removeClass('true');          $('.btn-submit.check').after(data);        }      });    } else {      M.toast({html: 'Похоже, у вас ошибка в ссылке на файл.'});      $('input[name="xml"]').addClass('error').removeClass('true');    }  });  function is_url(str) {    regexp =  /^(?:(?:https?|ftp):\/\/)?(?:(?!(?:10|127)(?:\.\d{1,3}){3})(?!(?:169\.254|192\.168)(?:\.\d{1,3}){2})(?!172\.(?:1[6-9]|2\d|3[0-1])(?:\.\d{1,3}){2})(?:[1-9]\d?|1\d\d|2[01]\d|22[0-3])(?:\.(?:1?\d{1,2}|2[0-4]\d|25[0-5])){2}(?:\.(?:[1-9]\d?|1\d\d|2[0-4]\d|25[0-4]))|(?:(?:[a-z\u00a1-\uffff0-9]-*)*[a-z\u00a1-\uffff0-9]+)(?:\.(?:[a-z\u00a1-\uffff0-9]-*)*[a-z\u00a1-\uffff0-9]+)*(?:\.(?:[a-z\u00a1-\uffff]{2,})))(?::\d{2,5})?(?:\/\S*)?$/;    if (regexp.test(str)) {      return true;    } else {      return false;    }  }  // open  $('.agency-right-info .line.phone span.show').on('click', function() {    var phone = $(this).data('phone');    console.log(phone);    var phonevalue = phone.replace(/[^+\d]/g, '');    phone = '<a href="tel:'+phonevalue+'">'+phone+'</a>';    $(this).html(phone).removeClass('show');  });});(function ($) { Drupal.behaviors.hh = { attach: function (context, settings) {  // if ($('.termin-main-box .slidercar').length > 0) {    // open gallery    $('#content-wrap').on('click', '.gallery-open.termin', function() {      var box = $(this).closest('.termin-main-box');      if (box.find('.item.tns-slide-active').length) {        var index = box.find('.item.tns-slide-active').attr('data-num');        box.find('.slidercar .item[data-num="'+index+'"]').trigger('click');      }    });    $('#content-wrap').on('click', '.termin-main-box.object .plan-view', function() {      $(this).closest('.termin-main-box').find('.planbox').click();    });  // }function getSorted(selector, attrName, type = 'asc') {  return $($(selector).toArray().sort(function(a, b) {    var aVal = parseInt(a.getAttribute(attrName)), bVal = parseInt(b.getAttribute(attrName));    if (type == 'asc') return aVal - bVal;    else if (type == 'desc') return bVal - aVal;  }));}// table sort$('.npc .fl-table .header-table').unbind('click').on('click', '.ts:not(.no-sort)', function() {  var type = $(this).data('type');  var sort = {'asc': 'desc', 'desc': 'asc'};  if ($(this).hasClass('active')) {    var sort = sort[$(this).attr('data-sort')];    $(this).attr('data-sort', sort);    $('.npc .fl-table .content-table').html(getSorted('.line-flat', 'data-'+type, sort));  } else {    $('.npc .fl-table .header-table .ts').removeClass('active');    $(this).attr('data-sort', 'asc').addClass('active');    $('.npc .fl-table .content-table').html(getSorted('.line-flat', 'data-'+type, 'asc'));  }});// scroll fast link$('div.linkspace').on('click', function(e){  var name = $(this).attr('href').replace(new RegExp("#",'gi'), '');  $('html, body').stop().animate({    scrollTop: $('div[id='+name+']').offset().top-80  }, 300);});// anim speedvar speed = 150;// termin complex gallery/*if ($('.slider-full .owl-carousel').length) {  $('.slider-full .owl-carousel').owlCarousel({    animateIn: 'fadeIn',    items: 1,    slideBy: 1,    loop: true,    margin: 4,    dots: false,    nav: true,    autoWidth: true,    center: true,    video: true,    onTranslate: function() {      $('.slider-full').find('iframe').each(function() {        $(this).attr('src', '');        return false;      });      setTimeout(function () {        var count = $('.owl-item.active.center .item').data('num');                var max = $('#photo-slider .bottom .info .count span.all').text();        if (count > max) count = 1;                $('#photo-slider .bottom .info .count span.current').html(count);      }, 10);