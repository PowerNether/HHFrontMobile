	} else {
		$('#filter-main .filter-field').addClass('active');
		$('.full-filter-main-box').show(time);
	}
	*/
	// count
	delay(function() { $.fn.ffilter('counter', {'count': 'update', 'pager': 'new', 'page': 'update', 'title': true}); }, delaytime);
});
