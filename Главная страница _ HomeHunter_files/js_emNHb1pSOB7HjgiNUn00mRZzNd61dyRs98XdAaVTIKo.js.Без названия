ive')) {    var cy = $(event.target).data('cy');    $(event.target).parent().find('.item').removeClass('active');    $(event.target).addClass('active');    $.cookie('cy', cy, { expires: 365, path: '/' });  }  */  if ($(event.target).closest('#filter-main .build-choseen.active *:not(input)').length == 0) {    $('#filter-main .build-choseen.active .listing').slideUp(500);    $('#filter-main .build-choseen.active').removeClass('active');  }  // Closed list select  if ($(event.target).closest('#filter-main .filter-field.select-list.active').length == 0) {    $('#filter-main .filter-field.select-list.active .list').slideUp(speed);    $('#filter-main .filter-field.select-list.active').removeClass('active');  }  // Closed list price  if ($(event.target).closest('#filter-main .filter-field.number-list.active *:not(input)').length == 0) {    $('#filter-main .filter-field.number-list.active .list').slideUp(speed);    $('#filter-main .filter-field.number-list.active').removeClass('active');    $('#filter-main .filter-field.number-list').attr('data-active', null);  }});// front$('#front-property .type-choose li').on('click', function() {  if (!$(this).hasClass('active')) {    $('#front-property .type-choose li').removeClass('active');    $(this).addClass('active');    var type = $(this).data('type');    $('#front-property .content').slideUp(speed);    $('#front-property .content[data-type="'+type+'"]').slideDown(speed);  }});// number list$('.number-list[data-type="price"] input').mask('9 999 999 999', {reverse: true});$('.number-list[data-type="area"] input').mask('99 999', {reverse: true});// gallery complexif ($('.pti .slider .owl-carousel').length > 0) {  $('.pti .slider .owl-carousel .owl-item:not(.cloned)').each(function () {  if ($(this).find('img').length > 0) {    $(this).attr('data-src', $(this).find('img').data('f'));  } else $(this).addClass('video');  });}// property gallery$('.pti .owl-carousel:not(.sc):not(.bs):not(.